{
  "name": "patrimoine-ambition",
  "version": "1.0.0",
  "description": "site vitrine de patrimoine-ambition.fr",
  "main": "app.js",
  "browserslist": [
    "last 10 versions"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:all": "npm-run-all --parallel dev watch:sass",
    "start:prod": "SET NODE_ENV=production&& nodemon server.js",
    "debug": "ndb server.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b browserslist public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch:js": "parcel watch public/js/index.js --out-dir public/js --out-file bundle.js",
    "build:js": "parcel build public/js/index.js --out-dir public/js --out-file bundle.js --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mm-devpro/patrimoine-ambition.git"
  },
  "keywords": [
    "patrimoine",
    "conseil",
    "immobilier",
    "famille",
    "argent",
    "placement",
    "bourse"
  ],
  "author": "Mickael Milliat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mm-devpro/patrimoine-ambition/issues"
  },
  "homepage": "https://github.com/mm-devpro/patrimoine-ambition#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.1",
    "fractional": "^1.0.0",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "nodemon": "^2.0.3",
    "parcel": "^1.12.4",
    "pug": "^2.0.4",
    "regenerator-runtime": "^0.13.5",
    "sharp": "^0.25.2",
    "slugify": "^1.4.0",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.0.6",
    "concat": "^1.0.3",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": "^12"
  }
}
